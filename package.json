{
  "name": "studykey",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "upload-audio": "node ./scripts/upload-to-cloudinary.js",
    "build:preview": "eas build --platform android --profile preview",
    "build:dev": "eas build --platform android --profile development",
    "build:debug": "eas build --platform android --profile debug",
    "build:production": "eas build --platform android --profile production",
    "build:ios-preview": "eas build --platform ios --profile preview",
    "build:ios-production": "eas build --platform ios --profile production",
    "build:all-preview": "eas build --platform all --profile preview",
    "build:all-production": "eas build --platform all --profile production",
    "build:local-android": "cd android && .\\gradlew.bat assembleRelease -x lint -x lintVitalAnalyzeRelease",
    "build:local-android-debug": "cd android && .\\gradlew.bat assembleDebug -x lint -x lintVitalAnalyzeRelease",
    "build:local-ios": "expo run:ios --configuration Release",
    "build:local-ios-debug": "expo run:ios --configuration Debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "cloudinary": "^2.7.0",
    "dotenv": "^17.2.2",
    "expo": "53.0.22",
    "expo-asset": "^11.1.7",
    "expo-av": "^15.1.7",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.6",
    "expo-speech": "~13.1.7",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "papaparse": "^5.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-redash": "^18.1.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-shadow-2": "^7.1.2",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "svg-path-properties": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "sharp": "^0.34.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
